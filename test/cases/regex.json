{
    "name": "Regex Library",
    "code": [
        "local Regex = Include(\"Regex\");",
        "local str1 = \"Hello, world!\";",
        "local str2 = \"Goodbye, world!\";",
        "local instance1 = new Regex::Instance(\"world\");",
        "local instance2 = new Regex::Instance(\"[A-Z].*!\");",
        "local instance3 = new Regex::Instance(\"bye\");",
        "PrintLn(instance1.PartialMatch(str1), instance1.PartialMatch(str2));",
        "PrintLn(instance2.FullMatch(str1), instance2.FullMatch(str2));",
        "PrintLn(instance3.PartialMatch(str1), instance3.PartialMatch(str2));",
        "PrintLn(instance1.Replace(str1, \"planet\"), instance1.Replace(str2, \"planet\"));",
        "local str3 = \"world hello world\";",
        "PrintLn(instance1.Replace(str3, \"planet\"), instance1.ReplaceAll(str3, \"planet\"));"
    ],
    "output": [
        "true true",
        "true true",
        "false true",
        "Hello, planet! Goodbye, planet!",
        "planet hello world planet hello planet"
    ]
}