{
    "name": "If-Statement",
    "code": [
        "local val = \"Hello, world!\";",
        "if (val == \"Hello, world!\") {",
        "   PrintLn(\"Hello\");",
        "}",
        "local function IfStatementTest(t, a = 0) {",
        "   if (t == 1) {",
        "      if (a == 1) {",
        "          Print(\"1\");",
        "      } else if (a == 2) {",
        "          Print(\"2\");",
        "      } else {",
        "          Print(\"3\");",
        "      }",
        "      PrintLn(\"A\");",
        "   } else if (t == 2) {",
        "      PrintLn(\"B\");",
        "   } else if (t == 3) {",
        "      PrintLn(\"C\");",
        "   } else if (t == 4) {",
        "       PrintLn(\"D\");",
        "   }",
        "}",
        "IfStatementTest(1, 1);",
        "IfStatementTest(1, 2);",
        "IfStatementTest(1, 3);",
        "IfStatementTest(1, 0);",
        "IfStatementTest(2);",
        "IfStatementTest(3);",
        "IfStatementTest(4);",
        "IfStatementTest(5);",
        "IfStatementTest(0);"
    ],
    "output": [
        "Hello",
        "1A",
        "2A",
        "3A",
        "3A",
        "B",
        "C",
        "D"
    ]
}