{
    "name": "Immediate Array",
    "code": [
        "local array = [ 10, 20, 30 ];",
        "local function TestPass1(arr, i = 0) {",
        "   return arr[i];",
        "}",
        "local function TestPass2(arr, i = 0) {",
        "   arr[i] = 10;",
        "}",
        "PrintLn(TestPass1(array, 1));",
        "array[1] = 5;",
        "PrintLn(TestPass1(array, 1));",
        "TestPass2(array, 1);",
        "PrintLn(TestPass1(array, 1));"
    ],
    "output": [
        "20",
        "5",
        "10"
    ]
}