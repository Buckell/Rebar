{
    "name": "Simple Immediate Table",
    "code": [
        "local table = {",
        "   hello = \"Hello!\",",
        "   [\"goodbye\"] = \"Goodbye!\",",
        "   [1] = 2",
        "};",
        "PrintLn(table.hello);",
        "PrintLn(table[\"goodbye\"]);",
        "PrintLn(table[1]);"
    ],
    "output": [
        "Hello!",
        "Goodbye!",
        "2"
    ]
}