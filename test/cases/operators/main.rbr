local table = {};
local a = 1;
local b = 2;

PrintLn("Index & Assignment:", table.a, table.a = 10, table.a);
table.b = 20;
PrintLn("Index:", table.b);
a = 10; b = 20; a = b;
PrintLn("Assignment:", a, b);
PrintLn("Equality:", a == b, b = 10, a == b);
PrintLn(8 | 2);
PrintLn(32 & 4);
PrintLn(8 ^ 2);
PrintLn(10 >| 2);
PrintLn(13 % 5);
PrintLn(true ? 10 : 20, false ? 10 : 20);
PrintLn(#"Hello, world!");

local arr = [ 10, 20, 30 ];
PrintLn(#arr);

local num = 4;
PrintLn(num, ++num, num++, num);
PrintLn(num, --num, num--, num);
PrintLn(~num);